-- Load the Library
local Library = loadstring(game:HttpGet('https://raw.githubusercontent.com/LuauCloud/Byte/refs/heads/main/Utils/Library.lua'))()

-- Create a Window
local Library_Window = Library.Add_Window('ikorz')

-- Create a Tab for "ikorz"
local Ikorz_Tab = Library_Window.Create_Tab({
    name = 'ikorz',
    icon = 'rbxassetid://'
})

-- Create a Section inside the Ikorz Tab
local Ikorz_Section = Ikorz_Tab.Create_Section()

-- Create the Auto Parry Dropdown Toggle
local Auto_Parry = Ikorz_Section.Create_DropToggle({
    name = 'Auto Parry',
    section = 'left',
    flag = 'Auto_Parry',
    options = {'Custom', 'Random', 'Backwards'},  -- Options for the dropdown
    callback = function(state)
        if state then
            print('ap - true')  -- Will print when Auto Parry is enabled
        else
            print('ap - false')  -- Will print when Auto Parry is disabled
        end
    end,
    callback2 = function(selected)
        print('Selected:', selected)  -- Will print the selected option from the dropdown
    end
})

-- Create a Tab for "Death Ball"
local Death_Ball_Tab = Library_Window.Create_Tab({
    name = 'Death Ball',
    icon = 'rbxassetid://'
})

-- Create a Section inside the Death Ball Tab
local Death_Ball_Section = Death_Ball_Tab.Create_Section()

-- Auto Parry Feature
local Auto_Parry_DB = Death_Ball_Section.Create_Toggle({
    name = 'Auto Parry',
    flag = 'Auto_Parry_DB',
    callback = function(state)
        if state then
            print('Death Ball Auto Parry activated')
        else
            print('Death Ball Auto Parry deactivated')
        end
    end
})

-- Visual Auto Parry Feature
local Visual_Auto_Parry_DB = Death_Ball_Section.Create_Toggle({
    name = 'Visual Auto Parry',
    flag = 'Visual_Auto_Parry_DB',
    callback = function(state)
        if state then
            print('Death Ball Visual Auto Parry activated')
        else
            print('Death Ball Visual Auto Parry deactivated')
        end
    end
})

-- Create a Tab for "Arsenal"
local Arsenal_Tab = Library_Window.Create_Tab({
    name = 'Arsenal',
    icon = 'rbxassetid://'
})

-- Create a Section inside the Arsenal Tab
local Arsenal_Section = Arsenal_Tab.Create_Section()

-- Aimbot Feature
local Aimbot = Arsenal_Section.Create_Toggle({
    name = 'Aimbot',
    flag = 'Aimbot',
    callback = function(state)
        if state then
            print('Aimbot activated')
        else
            print('Aimbot deactivated')
        end
    end
})

-- Silent Aimbot Feature
local Silent_Aimbot = Arsenal_Section.Create_Toggle({
    name = 'Silent Aimbot',
    flag = 'Silent_Aimbot',
    callback = function(state)
        if state then
            print('Silent Aimbot activated')
        else
            print('Silent Aimbot deactivated')
        end
    end
})

-- Triggerbot Feature
local Triggerbot = Arsenal_Section.Create_Toggle({
    name = 'Triggerbot',
    flag = 'Triggerbot',
    callback = function(state)
        if state then
            print('Triggerbot activated')
        else
            print('Triggerbot deactivated')
        end
    end
})

-- Wallhack Feature
local Wallhack = Arsenal_Section.Create_Toggle({
    name = 'Wallhack',
    flag = 'Wallhack',
    callback = function(state)
        if state then
            print('Wallhack enabled')
        else
            print('Wallhack disabled')
        end
    end
})

-- ESP (Extra Sensory Perception) Feature
local ESP = Arsenal_Section.Create_Toggle({
    name = 'ESP',
    flag = 'ESP',
    callback = function(state)
        if state then
            print('ESP enabled')
        else
            print('ESP disabled')
        end
    end
})
