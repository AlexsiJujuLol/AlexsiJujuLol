-- Load Byte UI Library
local Library = loadstring(game:HttpGet('https://raw.githubusercontent.com/LuauCloud/Byte/refs/heads/main/Utils/Library.lua'))()

-- Create the Main Window
local Library_Window = Library.Add_Window('Script Executor')

-- Create a Tab
local Script_Tab = Library_Window.Create_Tab({
    name = 'Script',
    icon = 'rbxassetid://'
})

-- Create a Section in the Tab
local Script_Section = Script_Tab.Create_Section()

-- Add a Button to Execute the Loadstring Script
local Execute_Script = Script_Section.Create_Button({
    name = 'Execute Script',
    callback = function()
        local scriptURL = "https://raw.githubusercontent.com/AlexsiJujuLol/Ikorz/main/AlexsiJujuLol/Ikorz.lua"
        local success, scriptContent = pcall(function()
            return game:HttpGet(scriptURL)
        end)

        if success then
            local successLoad, err = pcall(function()
                loadstring(scriptContent)()
            end)

            if successLoad then
                print('Script Executed Successfully!')
            else
                warn('Error Executing Script:', err)
            end
        else
            warn('Failed to Fetch Script')
        end
    end
})

-- Add a DropToggle for Auto Parry
local Auto_Parry = Script_Section.Create_DropToggle({
    name = 'Auto Parry',
    section = 'left', -- Align it to the left
    flag = 'Auto_Parry',

    options = {'Custom', 'Random', 'Backwards'},
    callback = function(state)
        if state then
            print('Auto Parry Enabled')
        else
            print('Auto Parry Disabled')
        end
    end,

    callback2 = function(selected)
        print('Selected Mode:', selected)
    end
})
