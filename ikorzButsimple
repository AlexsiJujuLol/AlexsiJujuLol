local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/LuauCloud/Byte/refs/heads/main/Utils/Library.lua"))()

local Library_Window = Library.Add_Window('Acceptions')
local Blatant_Tab = Library_Window.Create_Tab({name = 'Blatant', icon = 'rbxassetid://'})

local Blatant_Section = Blatant_Tab.Create_Section()

local Auto_Parry = Blatant_Section.Create_Toggle({
    name = 'Auto Parry',
    flag = 'Auto_Parry',
    callback = function(state)
        if state then
            -- Auto Parry Logic Here
        else
            -- Disable Auto Parry Logic Here
        end
    end
})

local Manual_Range_Spam = Blatant_Section.Create_Toggle({
    name = 'Spam Manual Range',
    flag = 'Manual_Range_Spam',
    callback = function(state)
        if state then
            -- Spam Manual Range Logic Here
        else
            -- Disable Spam Manual Range Logic Here
        end
    end
})

local scriptURL = "https://raw.githubusercontent.com/AlexsiJujuLol/Ikorz/main/Ikaz.lua"
loadstring(game:HttpGet(scriptURL))()
